<!doctype html>
<html>
<head>
  <title>{{#resume.basics}}{{name}}{{/resume.basics}}</title>
  <!-- Meta -->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/@mdi/font@5.8.55/css/materialdesignicons.min.css" rel="stylesheet">
  <!-- Font Awesome -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css" rel="stylesheet"/>
  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css2?family=Nunito:ital,wght@0,200;0,300;0,400;0,600;0,700;0,800;1,200;1,300;1,400;1,600;1,700;1,800&display=swap" rel="stylesheet"/>
  <!-- MDB -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.0.0/mdb.min.css" rel="stylesheet"/>
  <style>
      {{{css}}}
  </style>
  <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>

<body>
<div class="container-fluid p-0" style="background-image: url('https://github.com/irvanmohamad/irvanmohamad.github.io/raw/main/img/footer-bg.svg'); background-position: center bottom; background-repeat: no-repeat;">
  <header class="w-100" style="background-image: url('https://github.com/irvanmohamad/irvanmohamad.github.io/raw/main/img/banner-bg.svg'); background-position: center top; background-repeat: no-repeat; position: absolute; top: 0px; height: 1100px;"></header>
  <div class="container mt-5">
    <div class="row justify-content-center">
      {{#resume.basics}}
      <!-- BIG PHOTO -->
      <div class="col-xxl-4 col-xl-5 col-lg-6 col-md-8 text-center p-4">
        <div class="card h-100 rounded-xl shadow">
          <div class="card-body p-5 rounded-xl">
            <img src="{{picture}}" class="mb-4"/>
            <h3 class="mb-4">{{name}}</h3>
            <button id="big-title">{{label}}</button>
          </div>
        </div>
      </div>
      <!-- BASIC BIODATA -->
      <div class="col-xxl-4 col-xl-5 col-lg-6 col-md-8 p-4">
        <div class="card h-100 rounded-xl shadow">
          <div class="card-body p-5 rounded-xl">
            <span class="mdi mdi-baby-face icon-bio"></span>{{birth}}
            <br>
            <span class="mdi mdi-map-marker icon-bio"></span>{{location.region}}
            <br>
            <span class="mdi mdi-cellphone-iphone icon-bio"></span><a href="tel:{{phone}}">{{phone}}</a>
            <br>
            <span class="mdi mdi-email icon-bio"></span><a href="mailto:{{email}}">{{email}}</a>
            <hr>
            <span class="mdi mdi-earth icon-bio"></span><a href="{{url}}" target="_blank">{{displayUrl url}}</a>
            <br>
            {{#each profiles}}
              <span class="mdi mdi-{{toLowerCase network}} icon-bio"></span><a href="{{url}}" target="_blank">{{displayUrl url}}</a>
              <br>
            {{/each}}
          </div>
        </div>
      </div>
      {{/resume.basics}}
    </div>
    <div class="row justify-content-center">
      <!-- BEGIN OF SIDEBAR -->
      <div class="col-xxl-4 col-xl-5 col-lg-6 col-md-8 p-4">
        <div class="col mb-4">
          <div class="card bg-white rounded-xl">
            <div class="card-body p-5 rounded-xl">
              <img src="https://github.com/irvanmohamad/irvanmohamad.github.io/raw/main/img/graduate-student.png" class="d-inline rounded-xl mb-3 me-2" style="width: 4rem;"/>
              <h4 class="d-inline">PENDIDIKAN</h4>
              <ul>
                {{#resume.education}}
                  <li class="mb-3 ps-2">
                    <a href="{{url}}" target="_blank"><h6 class="no-bottom-margin">{{award studyType}}, {{area}} <span class="mdi mdi-link-variant"></span></h6></a>
                    {{institution}}<br>
                    <div class="subtitle">{{monthYear startDate}} - {{monthYear endDate}}</div>
                  </li>
                {{/resume.education}}
              </ul>
            </div>
          </div>
        </div>
        <div class="col mb-4">
          <div class="card bg-white rounded-xl">
            <div class="card-body p-5 rounded-xl">
              <img src="https://github.com/irvanmohamad/irvanmohamad.github.io/raw/main/img/certification.png" class="d-inline rounded-xl mb-3 me-2" style="width: 4rem;"/>
              <h4 class="d-inline">SERTIFIKASI</h4>
              <ul>
                {{#resume.awards}}
                  <li class="mb-3 ps-2">
                    <h6 class="no-bottom-margin">{{title}}</h6>
                    {{awarder}}<br>
                    <div class="subtitle">{{monthYear date}}</div>
                  </li>
                {{/resume.awards}}
              </ul>
            </div>
          </div>
        </div>
        <div class="col mb-4">
          <div class="card rounded-xl">
            <div class="card-body p-5 rounded-xl">
              <img src="https://github.com/irvanmohamad/irvanmohamad.github.io/raw/main/img/training.png" class="d-inline rounded-xl mb-3 me-2" style="width: 4rem;"/>
              <h4 class="d-inline">PELATIHAN</h4>
              <ul>
                {{#resume.training}}
                  <li class="mb-3 ps-2">
                    <h6 class="no-bottom-margin">{{title}}</h6>
                    {{trainer}}<br>
                    <div class="subtitle">{{monthYear date}}</div>
                  </li>
                {{/resume.training}}
              </ul>
            </div>
          </div>
        </div>
        <div class="col mb-4">
          <div class="card rounded-xl">
            <div class="card-body p-5 rounded-xl">
              <img src="https://github.com/irvanmohamad/irvanmohamad.github.io/raw/main/img/language.png" class="d-inline rounded-xl mb-3 me-2" style="width: 4rem;"/>
              <h4 class="d-inline">BAHASA</h4>
              <ul>
                {{#resume.languages}}
                  <li class="ps-2">{{language}} <div class="subtitle">({{fluency}})</div></li>
                {{/resume.languages}}
              </ul>
            </div>
          </div>
        </div>
        <div class="col mb-4">
          <div class="card rounded-xl">
            <div class="card-body p-5 rounded-xl">
              <img src="https://github.com/irvanmohamad/irvanmohamad.github.io/raw/main/img/interest.png" class="d-inline rounded-xl mb-3 me-2" style="width: 4rem;"/>
              <h4 class="d-inline">MINAT LAIN</h4>
              <ul>
                {{#resume.interests}}
                  <li class="ps-2">{{name}}</li>
                {{/resume.interests}}
              </ul>
            </div>
          </div>
        </div>
      </div>
      <!-- END OF SIDEBAR -->
      <!-- BEGIN OF MAIN WRAPPER -->
      <div class="col-xxl-8 col-xl-7 p-4">
        <div class="mb-4">
          <div class="card rounded-xl">
            <div class="card-body p-5 rounded-xl">
              <div class="col mb-4">
                <img src="https://github.com/irvanmohamad/irvanmohamad.github.io/raw/main/img/punch.png" class="d-inline me-4" style="width: 4rem;"/>
                <h2 class="d-inline">PENGALAMAN</h2>
              </div>
              <div class="mb-4 d-block" style="background-color: rgb(255, 104, 99); height: 2px; width: 4rem;"></div>
              {{#resume.work}}
                <div class="item-main-wrapper"> <!-- item margin -->
                  <div class="mb-3"> <!-- header margin -->
                    <div class="clearfix">
                      <h5 class="float-start no-bottom-margin">{{company}}</h5>
                      <div class="float-end subtitle mt-2">{{monthYear startDate}} - {{monthYear endDate}}</div>
                    </div>
                    <div class="mb-3 subtitle">{{position}}</div>
                  </div>
                  <div>{{#markdown}}{{summary}}{{/markdown}}</div>
                </div>
              {{/resume.work}}
            </div>
          </div>
        </div>
        <div class="col mb-4">
          <div class="card rounded-xl">
            <div class="card-body p-5 rounded-xl">
              <div class="mb-4">
                <img src="https://github.com/irvanmohamad/irvanmohamad.github.io/raw/main/img/type.png" class="d-inline me-4" style="width: 4rem;"/>
                <h3 class="d-inline">KEAHLIAN</h3>
              </div>
              <div class="mb-4 d-block" style="background-color: rgb(255, 104, 99); height: 2px; width: 4rem;"></div>
              {{#resume.skills}}
                <h6 class="d-inline-flex w-25" style="min-width: 90px;">{{name}}</h6>
                <div class="progress d-inline-flex w-50 rounded-pill" style="height: 6px;">
                  <div
                    class="progress-bar progress-bar-striped"
                    role="progressbar"
                    style="width: {{skillLevel level}};"
                    aria-valuenow="{{skillLevel level}}"
                    aria-valuemin="0"
                    aria-valuemax="100"
                  ></div>
                </div>
              {{/resume.skills}}
            </div>
          </div>
        </div>
        <div class="col mb-4">
          <div class="card rounded-xl">
            <div class="card-body p-5 rounded-xl">
              <div class="mb-4">
                <img src="https://github.com/irvanmohamad/irvanmohamad.github.io/raw/main/img/celebrate.png" class="d-inline me-4" style="width: 5rem;"/>
                <h3 class="d-inline">TOOLS</h3>
              </div>
              <div class="mb-4 d-block" style="background-color: rgb(255, 104, 99); height: 2px; width: 5rem;"></div>
              {{#resume.technology}}
                <div class="d-block mb-3">
                  <h6 class="d-inline-block w-25" style="min-width: 90px;">{{area}}</h6>
                  <div class="d-inline-block">
                    {{#each tools}}
                      <span class="badge bg-primary rounded-wd text-space">{{this}}</span>
                    {{/each}}
                  </div>
                </div>
              {{/resume.technology}}
            </div>
          </div>
        </div>
        <div class="col mb-4">
          <div class="card rounded-xl">
            <div class="card-body p-5 rounded-xl">
              <div class="mb-4">
                <img src="https://github.com/irvanmohamad/irvanmohamad.github.io/raw/main/img/smile.png" class="d-inline me-4" style="width: 4rem;"/>
                <h3 class="d-inline">PROYEK</h3>
              </div>
              <div class="mb-4 d-block" style="background-color: rgb(255, 104, 99); height: 2px; width: 4rem;"></div>
              {{#resume.projects}}
                <div class="item-main-wrapper"> <!-- item margin -->
                  <div class="mb-3"> <!-- header margin -->
                    <div class="clearfix">
                      <a href="{{url}}" target="_blank"><h5 class="float-start no-bottom-margin">{{name}} <span class="mdi mdi-link-variant"></span></h5></a>
                      <div class="float-end subtitle mt-2">{{monthYear startDate}} - {{monthYear endDate}}</div>
                    </div>
                    <div class="mb-3 subtitle">{{company}}</div><br>
                    <div class="mt-2 mb-1">
                      <span class="mdi mdi-account-multiple"></span>
                      <div class="d-inline-block">
                        {{#each job}}
                          <span class="badge bg-warning text-dark rounded-wd text-space">{{this}}</span>
                        {{/each}}
                      </div>
                    </div>
                    <div>
                      <span class="mdi mdi-tag-multiple"></span>
                      <div class="d-inline-block">
                        {{#each technology}}
                          <span class="badge bg-primary rounded-wd text-space">{{this}}</span>
                        {{/each}}
                      </div>
                    </div>
                  </div>
                  <div>{{#markdown}}{{summary}}{{/markdown}}</div>
                </div>
              {{/resume.projects}}
            </div>
          </div>
        </div>
        <div class="col mb-4 text-center" style="color: white">
          Made with <i class="mdi mdi-heart-multiple" style="color: rgb(255,50,50);"></i> by <a href="https://github.com/irvanmohamad/jsonresume-theme-bzdev" target="_blank">Mohamad Irvan Faradian</a> for <s>Lazy</s> Busy Developers
        </div>
      </div>
      <!-- END OF MAIN WRAPPER -->
    </div>
  </div>
</div>

<!-- MDB -->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.0.0/mdb.min.js"></script>
</body>

</html>